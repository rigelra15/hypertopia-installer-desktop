{
  "categories": [
    {
      "title": "## âœ¨ New Features",
      "labels": ["feat", "feature"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## ğŸš€ Improvements",
      "labels": ["perf", "refactor", "style"]
    },
    {
      "title": "## ğŸ“¦ Maintenance",
      "labels": ["chore", "build", "ci"]
    },
    {
      "title": "## ğŸ“ Documentation",
      "labels": ["docs"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test"]
    },
    {
      "title": "## ğŸ”„ Other Changes",
      "labels": []
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": "date",
  "template": "${{CHANGELOG}}\n\n${{UNCATEGORIZED}}",
  "pr_template": "- ${{TITLE}}\n${{BODY}}",
  "commit_template": "**${{TITLE}}**\n${{BODY}}\n",
  "empty_template": "No notable changes in this release.",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)(\\(.+\\))?:",
      "target": "feat",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^(fix|bug)(\\(.+\\))?:",
      "target": "fix",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^(perf|refactor|style)(\\(.+\\))?:",
      "target": "perf",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^(chore|build|ci)(\\(.+\\))?:",
      "target": "chore",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^(docs)(\\(.+\\))?:",
      "target": "docs",
      "method": "match",
      "on_property": "title"
    },
    {
      "pattern": "^(test)(\\(.+\\))?:",
      "target": "test",
      "method": "match",
      "on_property": "title"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "duplicate_filter": true
}
